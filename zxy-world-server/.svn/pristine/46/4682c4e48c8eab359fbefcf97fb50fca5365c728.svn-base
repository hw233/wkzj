<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="event.queue.size" class="java.lang.Integer">
		<constructor-arg value="50000" />
	</bean>
	
	<bean id="event.pool.size" class="java.lang.Integer">
		<constructor-arg value="5" />
	</bean>
	
	<bean id="event.pool.max.size" class="java.lang.Integer">
		<constructor-arg value="30" />
	</bean>
	
	<bean id="event.pool.alive.time" class="java.lang.Integer">
		<constructor-arg value="60" />
	</bean>
</beans>