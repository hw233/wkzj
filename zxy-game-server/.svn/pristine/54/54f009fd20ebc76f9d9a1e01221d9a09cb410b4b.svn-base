package com.jtang.gameserver.module.notify.type;

/**
 * 感谢盟友的类型
 * @author 0x737263
 *
 */
public enum ThankAllyType {
	
	/**
	 * 无(默认值用)
	 */
	NONE(0),

	/**
	 * 合作关卡领取声望/物品时
	 */
	STORY(1),

	/**
	 * 试炼洞领取声望/物品时
	 */
	TRAIL(2),
	
	/**
	 * 收礼时
	 */
	ACCEPT_GIFT(3),
	
	/**
	 * 收到天才地宝赠送的装备
	 */
	ACCEPT_GIVE_EQUIP(4);
	
	private int id;
	
	private ThankAllyType(int id) {
		this.id =  id;
	}
	
	public int getId() {
		return id;
	}
}
