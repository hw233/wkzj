package com.jtang.gameserver.module.buffer.type;

/**
 * 枚举buffer失效的类型
 * @author vinceruan
 *
 */
public interface BufferEndType {

	/**
	 * 跳到一次后失效
	 */
	int EFFECT_ONCE = 1;

	/**
	 * 当前轮结束时失效 
	 */
	int ROUND_END = 2;
	
	/**
	 * 下一轮结束时失效 
	 */
	int NEXT_ROUND_END = 4;

	/**
	 * 死亡时失效 
	 */
	int TARGET_DIE = 8;

	/**
	 * 战斗结束时失效
	 */
	int FIGHT_END = 16;
	
	/**
	 * 施法者死亡
	 */
	int CASTER_DIE = 32;
}
