#server state 0.正常状态（所有人可访问） 1.维护状态（允许ip列表访问） 2.关闭状态（所有人不可访问）
maintain=0

#debugmode 调试模式（true：不做token验证）
debug_mode=false

#game sever id
server_id=1

#platform id (不填则开放所有) 1.开发测试     11.嘉堂    12.91  13.uc  14.小米  16.百度云  17.360 18.多酷  19.当乐  20.pp助手 21.oppo可可 22.机锋网 23.豌豆荚 24.安智
platform_id=

#客户端连接监听端口
gateway_port=10123

#gap管理接口监听端口
admin_port=9124

#维护状态时，哪些ip可以访问 gateway_port 例如(半角逗号分隔)：172.24.0.21,172.24.0.22,...
maintain_ip=218.17.158.211

#允许哪些 ip可访问admin_port端口   例如(半角逗号分隔)：172.24.0.21,172.24.0.22,...
admin_ip=

#debug_uid
debug_uid=

#是否启动世界服
world_server_enable=false

#每x毫秒频率重连世界服
world_server_reconnect_time=1000

#世界服连接ip
world_server_ip=127.0.0.1

#世界服连接端口
world_server_port=8123

#技能调试开关 
skill_debug=false
