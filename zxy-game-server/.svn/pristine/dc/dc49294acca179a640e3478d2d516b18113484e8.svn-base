package com.jtang.gameserver.module.gift.model;

/**
 * 下发礼物的状态
 * @author vinceruan
 *
 */
public interface GiftStateType {
	/**
	 * 已送出
	 */
	byte GIVEN = 1;
	
	/**
	 * 已收到
	 */
	byte RECEIVED = 2;
	
	/**
	 * 已经接受
	 */
	byte ACCEPTED = 3;
	
	/**
	 * 大礼包的状态,对应的值：
	 * 0:代表今天未领取
	 * 1：代表今天已领取
	 */
	byte GIFT_PACKAGE_STATE = 4;
}
