package com.jtang.gameserver.module.adventures.achievement.type;

public enum AchieveAttributeKey {
	
	/**
	 * 成就状态，0未完成，1完成，2已领取奖励
	 */
	ACHIEVE_STATE(1),
	
	/**
	 * 达成的条件
	 */
	ACHIEVE_CONDITIONS(2);
	
	private byte code;
	private AchieveAttributeKey(int code){
		this.code = (byte)code;
	}
	
	public byte getCode(){
		return code;
	}
}
