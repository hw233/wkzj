<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean id="httputil.connection.timeout" class="java.lang.Integer">
		<constructor-arg value="5000" />
	</bean>
	<bean id="httputil.maxtotal" class="java.lang.Integer">
		<constructor-arg value="256" />
	</bean>
	<bean id="httputil.so.timeout" class="java.lang.Integer">
		<constructor-arg value="5000" />
	</bean>
	<bean id="httputil.useproxy" class="java.lang.Boolean">
		<constructor-arg value="false" />
	</bean>
	<bean id="httputil.proxy.ip" class="java.lang.String">
		<constructor-arg value="172.24.0.254" />
	</bean>
	<bean id="httputil.proxy.port" class="java.lang.Integer">
		<constructor-arg value="8088" />
	</bean>
	<bean id="httputil.login.validate_url" class="java.lang.String">
		<constructor-arg value="http://login.wk.qianz.com/OAuthLogin/Verify" />
	</bean>
</beans>