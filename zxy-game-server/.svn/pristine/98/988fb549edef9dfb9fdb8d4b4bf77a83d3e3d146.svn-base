package com.jtang.gameserver.module.adventures.achievement.type;

/**
 * 成就进行的状态
 * @author pengzy
 *
 */
public enum AchieveState {
	/**
	 * 未达成
	 */
	UN_ACHIEVED(0),
	
	/**
	 * 达成
	 */
	AHIEVED(1),
	
	/**
	 * 已领取奖励
	 */
	GETED(2);
	
	private byte state;
	
	private AchieveState(int state){
		this.state = (byte)state;
	}
	public byte getState(){
		return state;
	}
}
